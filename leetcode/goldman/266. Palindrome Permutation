class Solution:
    def canPermutePalindrome(self, s: str) -> bool:
        d = {}
        for i in s:
            if i in d:
                d[i]+=1
            else:
                d[i]=1
        single=1
        for key,value in d.items():
            if value % 2 ==1:
                single-=1
        
        if single<0:
            return False
        else:
            return True
