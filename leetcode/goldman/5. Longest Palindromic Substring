class Solution:
    def longestPalindrome(self, s: str) -> str:
        n=len(s)
        if n <2:
            return s
        dp=[[False]*n for _ in range(n)]      
        start,end,maxL =0,0,0
        res=""
        for i in range(n):
            for j in range(i+1):
                if s[i] == s[j] and (i-j <= 2 or dp[i-1][j+1]):
                    dp[i][j]= True
                    if i-j+1 > maxL:
                        maxL=i-j+1
                        res=s[j:i+1]
        return res
