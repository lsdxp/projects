class Solution:
    def minSubArrayLen(self, target: int, nums: List[int]) -> int:
        start=end=0
        l=math.inf
        sum=0
        while end < len(nums):
            sum+=nums[end]
            end+=1
            while sum>=target:
                l=min(l,end-start)
                sum-=nums[start]
                start+=1
        return  0 if l == math.inf else l
