class Solution:
    def maximalSquare(self, matrix: List[List[str]]) -> int:
        m,n=len(matrix),len(matrix[0])
        dp=[[math.inf]*n for _ in range(m)]
        maxv=0   
        for i in range(m):
            for j in range(n):
                if matrix[i][j] == "1":
                    if i == 0 or j == 0:
                        dp[i][j]=1
                    else:     
                        dp[i][j]=min(dp[i-1][j],dp[i-1][j-1],dp[i][j-1])+1
                    if maxv<dp[i][j]:
                        maxv=dp[i][j]
                else:
                    dp[i][j]=0
        return maxv*maxv
