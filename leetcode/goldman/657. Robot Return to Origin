class Solution:
    def judgeCircle(self, moves: str) -> bool:
        horizontal=0
        vertical=0
        v={"U":1,"D":-1}
        h={"L":1,"R":-1}

        for i in moves:
            if i in v:
                vertical+=v[i]
            if i in h:
                horizontal+=h[i]
                
        if horizontal ==0 and vertical == 0:
            return True
        return False
