class Solution:
    def lengthOfLIS(self, nums: List[int]) -> int:
        n=len(nums)
        dp=[1]*n
        maxv=1
        for i in range(1,n):
            for j in range(0,i):
                if nums[j] < nums[i]:
                    dp[i]=max(dp[j]+1,dp[i])  
            if maxv < dp[i]:
                maxv=dp[i]
        return maxv
