class Solution:
    def minPathSum(self, grid: List[List[int]]) -> int:
        import math
        m=len(grid)
        n=len(grid[0])
        dp=[[math.inf]*n for _ in range(m)]
        dp[0][0]=grid[0][0]
        for i in range(1,m):  
            dp[i][0]=dp[i-1][0]+grid[i][0]
        for j in range(1,n):  
            dp[0][j]=dp[0][j-1]+grid[0][j]         
        for i in range(1,m):
            for j in range(1,n):
                #print(i-1,j,dp[i-1][j],i,j-1,dp[i][j-1])
                dp[i][j]=min(dp[i-1][j],dp[i][j-1])+grid[i][j]
                #print(i,j,dp[i][j])
        
        return dp[m-1][n-1]
